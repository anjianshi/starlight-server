{
  "name": "starlight-server",
  "version": "1.4.0",
  "description": "Simple But Powerful Node.js HTTP Server",
  "type": "module",
  "scripts": {
    "dev": "rimraf dist && (concurrently --raw \"tsc -w\" \"sleep 5 && tsc-alias -w\" \"sleep 6 && nodemon dist/demo/index.js\")",
    "build": "rimraf dist && tsc && tsc-alias",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web",
    "app",
    "http",
    "framework"
  ],
  "homepage": "https://github.com/anjianshi/starlight-server",
  "bugs": {
    "url": "https://github.com/anjianshi/starlight-server/issues",
    "email": "anjianshi@gmail.com"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "license": "MIT",
  "author": "anjianshi <anjianshi@gmail.com>",
  "repository": "github:anjianshi/starlight-server",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@anjianshi/utils": "^1.3.1",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "swagger-ui-dist": "^5.11.10"
  },
  "devDependencies": {
    "@anjianshi/presets-eslint-node": "^1.0.3",
    "@anjianshi/presets-eslint-typescript": "^1.0.3",
    "@anjianshi/presets-prettier": "^1.0.0",
    "@anjianshi/presets-typescript": "^1.0.1",
    "@types/debug": "^4.1.9",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.6",
    "@types/swagger-ui-dist": "^3.30.4",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.2",
    "rimraf": "^4.3.0",
    "tsc-alias": "^1.8.8"
  },
  "eslintIgnore": [],
  "prettier": "@anjianshi/presets-prettier/prettierrc"
}
